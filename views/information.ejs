<div style="width: 100%; height: 844px; background-color: #2d2d2d; padding-top: 20px;">
    <section>
        <article>
            <div class="infor_article1">
                <div class="infor_image"><img style="width: 75px; height: 75px;" src="images/Group 32.png" alt="infor"></div>
                <div class="my_name">끝까지 달리는 러너,<span style="font-weight: 700; color: #66FFE3;">&nbsp;<%=user.nickname %></span> 님</div>

                <div class="gauge">
                    <progress class="progress" id="progress" value="60" min="0" max="100"></progress>
                </div>
            </div>
        </article>
        <hr style="width: 388px; border: 1px solid #6D6D6D">
        <article>
            <div class="infor_nav">
                <div style="margin-left: 20px; font-weight: 600; font-size: 18px; color: #A4E3D8;">나의 러너 프로필</div>
                <div class="my_profile" style="height: 126px;">
                    <div class="my_profile_detail">
                        <div>거주지</div>
                        <div class="my_profile_infor" data-field="address">
                            <%=user.address %>
                        </div>
                    </div>
                    <div class="my_profile_detail">
                        <div>러닝 지역</div>
                        <div style="display: flex;">
                            <div class="my_profile_infor" data-field="runningArea">
                                <%=user.runningArea%>
                            </div>
                        </div>
                    </div>
                    <div class="my_profile_detail">
                        <div>목표 거리</div>
                        <div class="my_profile_infor" data-field="goalDistance">
                            <%=user.goalDistance%>
                        </div>
                    </div>
                    <div class="my_profile_detail">
                        <div>성별</div>
                        <div class="my_profile_infor" data-field="gender">
                            <% if (user.gender === 'Male') { %>
                                남성
                            <% } else if (user.gender === 'Female') { %>
                                여성
                            <% } else { %>
                                기타
                            <% } %>
                        </div>
                    </div>
                </div>
            </div>
            <div class="my_profile" style="height: 126px;">
                <div class="my_profile_detail">
                    <div>최고기록</div>
                    <div class="my_profile_infor">xx km</div>
                </div>
                <div class="my_profile_detail">
                    <div>평균기록</div>
                    <div>
                        <div class="my_profile_infor">xx km</div>
                    </div>
                </div>
                <div class="my_profile_detail">
                    <div>목표기록</div>
                    <div class="my_profile_infor">xx km</div>
                </div>
                <div class="my_profile_detail">
                    <div>누적 거리</div>
                    <div class="my_profile_infor">xx km</div>
                </div>
            </div>
            <div class="my_profile" style="height: 40px;">
                <div class="my_profile_detail">
                    <div>나의 글</div>
                    <div class="my_profile_infor" data-action="mypost">나의 글 불러오기</div>
                </div>
            </div>
            <div class="my_profile" style="height: 40px;">
                <div class="my_profile_detail">
                    <div>프로필 편집하기</div>
                    <div class="my_profile_infor" data-action="editProfile">편집하기</div>
                </div>
            </div>
        </div>
    </section>
</div>